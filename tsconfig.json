{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": false,
    "jsx": "react",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "noImplicitAny": false,
    "noImplicitUseStrict": false,
    "removeComments": false,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./lib/"
  },
  "filesGlob": [
    "./ts/**/*.ts",
    "!./node_modules/**/*.ts",
    "./typings/**/*.ts"
  ],
  "compileOnSave": true,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "./ts/blueprintCatalog.ts",
    "./ts/dictionary.ts",
    "./ts/index.ts",
    "./ts/mixinCatalog.ts"
  ]
}
